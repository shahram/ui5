<mvc:View
    controllerName="de.jlabs.demo.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

<Button
    text="increment by 1"
    press=".incrementBy1"/>
    
<Text id="counter"  text="0"/>

<List
    id="episodeOverview"
    headerText="{/title}"
    items="{/episodes}">
    <items>
        <StandardListItem
            title="{title}" 
            description="{index}"
        />
    </items>
</List>



</mvc:View>